{
    "manifest_version": 3,
    "name": "EmojifyMe",
    "version": "1.0",
    "description": "Convert text to emoji art.",
    "permissions": ["activeTab"],
    "action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "EmojifyMe@Auraaid.netlify.com",
        "strict_min_version": "42.0"
      }
    }
  }
  